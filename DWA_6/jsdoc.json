{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["src", "BookConnect"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs), BookConnect/data.js"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./jsdoc",
    "recurse": true,
    "readme": "./readme.md",
    "template": "custom"
  }
}
